
@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Temperature</th>
            <th>Wind Speed m/s</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.WeatherList)
        {
        <tr>
            <td>@item.Name</td>
            <td>@item.Main.Temp</td>
            <td>@item.Wind.Speed</td>
        </tr>
        }
    </tbody>
</table>

